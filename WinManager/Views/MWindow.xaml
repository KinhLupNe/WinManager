<Window x:Class="WinManager.Views.MWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:views="clr-namespace:WinManager.Views"
        Title="Giám sát tài nguyên" Height="450" Width="800">
    <Grid>
        <TabControl>
            <TabItem Header="CPU">
                <views:CpuControl/>
            </TabItem>
            <TabItem Header="RAM">
                <views:RamControl/>
            </TabItem>
            <TabItem Header="DISK">
                <views:DiskControl/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
